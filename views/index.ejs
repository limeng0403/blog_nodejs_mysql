<%include header.ejs%>

<div class="container">
	<div class="row">
		<div class="col-xs-12">
			<h1><%=title%></h1>
			<p>Welcome to <%=title%></p>
		</div>
		<%=sqlResult.length%>
		<table class="table table-bordered">
			<thead>
				<tr>
					<th>id</th>
					<th>user</th>
					<th>name</th>
					<th>age</th>
					<th>pwd</th>
					<th>create date</th>
				</tr>
			</thead>
			<tbody>
				<%for(var i=0;i<sqlResult.length;i++){%>
				<tr>
					<td><%=sqlResult[i].id%></td>
					<td><%=sqlResult[i]['user']%></td>
					<td><%=sqlResult[i]['name']%></td>
					<td><%=sqlResult[i]['age']%></td>
					<td><%=sqlResult[i]['pwd']%></td>
					<td><%=sqlResult[i]['create_date']%></td>
				</tr>
				<%}%>
			</tbody>
		</table>
	</div>
</div>

<%include footer.ejs%>
